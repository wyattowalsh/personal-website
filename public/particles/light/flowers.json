        {
            "maxWidth": 480,
            "options": {
                "particles": {
                    "number": {
                        "value": 40
                    },
                    "move": {
                        "speed": 1
                    },
                    "size": {
                        "value": {
                            "min": 4,
                            "max": 10
                        }
                    }
