{
    "autoPlay": true,
    "background": {
        "color": {
            "value": "#f9f5ff"
        },
        "image": "",
        "position": "50% 50%",
        "repeat": "no-repeat",
        "size": "cover"
    },
    "particles": {
        "number": {
            "value": 120,
            "density": {
                "enable": true,
                "area": 800
            }
        },
        "color": {
            "value": ["#ff69b4", "#ff1493", "#ff00ff", "#ffc0cb", "#f500a0", "#e0e0f8", "#d5d9ff", "#8a2be2", "#ffd700", "#ff4500"]
        },
        "shape": {
            "type": ["circle", "image", "polygon"],
            "image": {
                "src": "../../flower.webp",
                "width": 60,
                "height": 60
            },
            "polygon": {
                "nb_sides": 6
            }
        },
        "opacity": {
            "value": {
                "min": 0.4,
                "max": 0.9
            },
            "random": true,
            "animation": {
                "enable": true,
                "speed": 0.8,
                "minimumValue": 0.4,
                "sync": false
            }
        },
        "size": {
            "value": {
                "min": 8,
                "max": 16
            },
            "random": true,
            "animation": {
                "enable": true,
                "speed": 4,
                "minimumValue": 8,
                "sync": false
            }
        },
        "move": {
            "enable": true,
            "speed": 2,
            "direction": "bottom",
            "random": false,
            "straight": false,
            "outModes": {
                "default": "out"
            },
            "attract": {
                "enable": false,
                "rotateX": 800,
                "rotateY": 1600
            }
        },
        "links": {
            "enable": true,
            "distance": 120,
            "color": "#ff69b4",
            "opacity": 0.5,
            "width": 1
        }
    },
    "interactivity": {
        "events": {
            "onHover": {
                "enable": true,
                "mode": ["bubble", "grab"],
                "parallax": {
                    "enable": true,
                    "force": 50,
                    "smooth": 8
                }
            },
            "onClick": {
                "enable": true,
                "mode": ["push", "repulse"]
            },
            "resize": true
        },
        "modes": {
            "grab": {
                "distance": 180,
                "links": {
                    "opacity": 0.8
                }
            },
            "bubble": {
                "distance": 180,
                "size": 12,
                "duration": 2,
                "opacity": 0.7,
                "mix": false
            },
            "repulse": {
                "distance": 120,
                "duration": 0.6
            },
            "push": {
                "quantity": 3
            },
            "remove": {
                "quantity": 2
            }
        }
    },
    "detectRetina": true,
    "fpsLimit": 60,
    "pauseOnBlur": true,
    "smooth": true,
    "responsive": [
        {
            "maxWidth": 768,
            "options": {
                "particles": {
                    "number": {
                        "value": 60
