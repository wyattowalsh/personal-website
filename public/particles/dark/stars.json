{
  "autoPlay": true,
  "background": {
    "color": {
      "value": "#0d0d1f"
    },
    "position": "50% 50%",
    "repeat": "no-repeat",
    "size": "cover",
    "opacity": 1
  },
  "fullScreen": {
    "enable": true,
    "zIndex": -1
  },
  "detectRetina": true,
  "fpsLimit": 60,
  "particles": {
    "number": {
      "value": 200,
      "density": {
        "enable": true,
        "area": 1000
      }
    },
    "color": {
      "value": ["#ffffff", "#ffd700", "#ffa07a", "#87cefa", "#fffacd"]
    },
    "shape": {
      "type": "star"
    },
    "opacity": {
      "value": {
        "min": 0.2,
        "max": 0.8
      },
      "animation": {
        "enable": true,
        "speed": 0.8,
        "minimumValue": 0.2,
        "sync": false
      }
    },
    "size": {
      "value": {
        "min": 2,
        "max": 5
      },
      "animation": {
        "enable": true,
        "speed": 4,
        "minimumValue": 1.5,
        "sync": false
      }
    },
    "rotate": {
      "value": 0,
      "random": true,
      "direction": "random",
      "animation": {
        "enable": true,
        "speed": 3,
        "sync": false
      }
    },
    "move": {
      "enable": true,
      "speed": {
        "min": 0.3,
        "max": 0.6
      },
      "direction": "none",
      "outModes": {
        "default": "out"
      },
      "trail": {
        "enable": true,
        "length": 12,
        "fill": {
          "color": "#0d0d1f"
        }
      }
    },
    "life": {
      "count": 0,
      "duration": {
        "value": 8,
        "random": {
          "enable": true,
          "minimumValue": 3
        }
      },
      "delay": {
        "value": 0.5,
        "random": {
          "enable": true,
          "minimumValue": 0.3
        }
      }
    },
    "twinkle": {
      "particles": {
        "enable": true,
        "frequency": 0.02,
        "opacity": 0.9
      }
    },
    "zIndex": {
      "value": {
        "min": 0,
        "max": 150
      }
    }
  },
  "interactivity": {
    "events": {
      "onHover": {
        "enable": true,
        "mode": "bubble"
      },
      "onClick": {
        "enable": true,
        "mode": ["repulse", "bubble"]
      },
      "resize": true
    },
    "modes": {
      "repulse": {
        "distance": 250,
        "duration": 0.8,
        "speed": 3,
        "maxSpeed": 50,
        "easing": "ease-out-quad"
      },
      "bubble": {
        "distance": 300,
        "size": 8,
        "duration": 2,
        "opacity": 0.9,
        "mix": false
      }
    }
  }
}